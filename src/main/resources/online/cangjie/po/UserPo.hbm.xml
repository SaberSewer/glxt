<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping SYSTEM "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >
<hibernate-mapping>
	<class name="online.cangjie.po.UserPo" table="user">
		<id name="id">
			<column name="id"></column>
			<generator class="native"></generator>
		</id>
		<property name="name">
			<column name="user_name"></column>
		</property>
		<property name="sex">
			<column name="user_sex"></column>
		</property>
		<property name="phone">
			<column name="user_phone"></column>
		</property>
		<property name="email">
			<column name="user_eamil"></column>
		</property>
		<property name="address">
			<column name="user_address"></column>
		</property>
		<property name="join" type="date">
			<column name="user_join"></column>
		</property>
		<property name="level">
			<column name="user_level"></column>
		</property>
		<property name="state">
			<column name="user_state"></column>
		</property>
	</class>
</hibernate-mapping>
